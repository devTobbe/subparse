{
  "name": "@devtobbe/SubParse",
  "version": "0.0.1",
  "description": "SubParse is a TypeScript library that converts different subtitle files into a standardized JSON object.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "npm run build && changeset publish",
    "lint": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "keywords": [
    "subtitles",
    "parsing",
    "json",
    "srt",
    "vtt",
    "typescript"
  ],
  "author": "devTobbe",
  "license": "MIT",
  "dependencies": {
    "cjs": "^0.0.11",
    "esm": "^3.2.25",
    "tsup": "^8.2.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
