{
  "name": "SubParse",
  "version": "1.0.0",
  "description": "SubParse is a TypeScript library that converts different subtitle files into a standardized JSON object.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc"
  },
  "keywords": [
    "subtitles",
    "parsing",
    "json",
    "srt",
    "vtt",
    "typescript"
  ],
  "repository": {
    "url": "https://github.com/devTobbe/subparse"
  },
  "author": "devTobbe",
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.4.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "tsup": "^8.2.4"
  }
}
